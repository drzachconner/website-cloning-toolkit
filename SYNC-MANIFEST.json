  "parameterized": {
    "description": "Tier 2: Files with site-specific hardcoded strings. After Phase 2 parameterization, most move to Tier 1.",
    "files": [
      {
        "path": "public/_headers",
        "reason": "CSP connect-src contains site-specific agent URL",
        "phase2_target": "parameterized"
      },
      {
        "path": "public/robots.txt",
        "reason": "Sitemap URL and Host directive are site-specific",
        "phase2_target": "parameterized"
      },
      {
        "path": "wrangler.toml",
        "reason": "Project name is site-specific (changes once per site)",
        "phase2_target": "parameterized"
      },
      {
        "path": ".github/workflows/deploy.yml",
        "reason": "Project name, notification email are site-specific",
        "phase2_target": "parameterized"
      },
      {
        "path": "public/llms.txt",
        "reason": "All content is site-specific",
        "phase2_target": "parameterized"
      },
      {
        "path": "package.json",
        "reason": "name field differs per site, dependencies should stay in sync",
        "phase2_target": "parameterized"
      },
      {
        "path": "public/site.webmanifest",
        "reason": "Site name, short_name, theme_color are site-specific",
        "phase2_target": "parameterized"
      }
    ]
  },

